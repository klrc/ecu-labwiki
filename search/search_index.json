f1] \u4ea7\u54c1\u9500\u552e\u5408\u540c \u00b6 \u7532\u65b9\uff1a[\u81ea\u5df1/\u5bfc\u5e08\u5747\u53ef\uff0c\u53ef\u54a8\u8be2\u5bf9\u65b9] \u9884\u7ea6\u501f\u6b3e\u5355 \u00b6 \uff08\u8be5\u6761\u76ee\u9700\u8981\u8865\u5145\uff09 Note \u7b7e\u5b57\u76d6\u7ae0\u987a\u5e8f\uff1a \u9884\u7ea6\u501f\u6b3e\u5355\uff1a\u5bfc\u5e08 \u5408\u540c\u5ba1\u6279\u8868\uff1a\u9662\u957f-\u4fe1\u606f\u529e-\u8bbe\u5907\u5904 \u9500\u552e\u5408\u540c\uff1a\u4f9b\u5e94\u5546\u76d6\u7ae0\u90ae\u5bc4-\u8bbe\u5907\u5904-\u5bc4\u56de\u4e00\u4efd \u8be6\u7ec6\u5185\u5bb9\u53ef\u89c1 2.\u5ba1\u6279\u6d41\u7a0b","title":"1. \u6750\u6599\u51c6\u5907"},{"location":"flow_server_a/#_1","text":"\u7528\u9879\u76ee\u7ecf\u8d39\u8d2d\u4e70\u670d\u52a1\u5668\uff0c\u4f60\u81f3\u5c11\u9700\u8981\uff1a \u534e\u4e1c\u7406\u5de5\u5927\u5b66\u7269\u8d44\u7c7b\u91c7\u8d2d\u5408\u540c\u5ba1\u6279\u8868 \u4f9b\u5e94\u5546\u63d0\u4f9b\u7684\u9500\u552e\u5408\u540c \u534e\u4e1c\u7406\u5de5\u5927\u5b66\u9884\u7ea6\u501f\u6b3e\u5355 \u8bf7\u63d0\u9192\u5bf9\u65b9\u51c6\u5907\u81f3\u5c113\u4efd\u9500\u552e\u5408\u540c \u4f9b\u5e94\u5546\u548c\u88c5\u5907\u5904\u4f1a\u5404\u6536\u8d70\u4e00\u4efd\uff0c\u7559\u4e00\u4efd\u5907\u6848\u3002","title":"\u8868\u683c\u6e05\u5355"},{"location":"flow_server_a/#_2","text":"","title":"\u586b\u5199"},{"location":"flow_server_a/#_3","text":"\u9879\u76ee\u540d\u79f0\uff1a[\u9700\u8981\u8d2d\u4e70\u7684\u4e1c\u897f] \u9879\u76ee\u8d1f\u8d23\u4eba\uff1a[\u5bfc\u5e08] \u8054\u7cfb\u4eba\uff1a[\u53ef\u586b\u81ea\u5df1]","title":"\u534e\u4e1c\u7406\u5de5\u5927\u5b66\u7269\u8d44\u7c7b\u91c7\u8d2d\u5408\u540c\u5ba1\u6279\u8868"},{"location":"flow_server_a/#_4","text":"\u7532\u65b9\uff1a[\u81ea\u5df1/\u5bfc\u5e08\u5747\u53ef\uff0c\u53ef\u54a8\u8be2\u5bf9\u65b9]","title":"\u4ea7\u54c1\u9500\u552e\u5408\u540c"},{"location":"flow_server_a/#_5","text":"\uff08\u8be5\u6761\u76ee\u9700\u8981\u8865\u5145\uff09 Note \u7b7e\u5b57\u76d6\u7ae0\u987a\u5e8f\uff1a \u9884\u7ea6\u501f\u6b3e\u5355\uff1a\u5bfc\u5e08 \u5408\u540c\u5ba1\u6279\u8868\uff1a\u9662\u957f-\u4fe1\u606f\u529e-\u8bbe\u5907\u5904 \u9500\u552e\u5408\u540c\uff1a\u4f9b\u5e94\u5546\u76d6\u7ae0\u90ae\u5bc4-\u8bbe\u5907\u5904-\u5bc4\u56de\u4e00\u4efd \u8be6\u7ec6\u5185\u5bb9\u53ef\u89c1 2.\u5ba1\u6279\u6d41\u7a0b","title":"\u9884\u7ea6\u501f\u6b3e\u5355"},{"location":"flow_server_b/","text":"\u6d41\u7a0b\u5ba1\u6279 \u00b6 \u8fd9\u4e00\u6b65\u53ef\u80fd\u9700\u8981\u82b1\u8d39\u8d85\u8fc7\u4e00\u5929\u7684\u65f6\u95f4\uff0c\u8bf7\u5408\u7406\u5b89\u6392\u65e5\u7a0b\u3002 \u8bf7\u6309\u4e0b\u9762\u7684\u6b65\u9aa4\u4f9d\u6b21\u529e\u7406\uff0c \u8d8a\u7ea7\u5ba1\u6838 \u662f\u5728\u5404\u4e2a\u5730\u65b9\u90fd\u662f\u4e0d\u88ab\u5141\u8bb8\u7684\u3002 \u9662\u957f\u7b7e\u5b57 \u00b6 \u627e\u949f\u9662\u957f\uff0c\u7814\u7a76\u751f\u697c9\u697c\u5de6\u624b\u8fb9\u6700\u91cc\u9762\uff0c\u5de6\u624b\u8fb9\u3002 9\u697c\u975e\u8ba1\u7b97\u673a\u4e13\u4e1a\uff0c\u8bbe\u6709\u95e8\u7981\u3002 \u4e0d\u505c\u5730\u6309\u95e8\u94c3\u53ef\u4ee5\u89e3\u51b3\u3002 \u9700\u8981\u6ce8\u610f\uff0c\u949f\u8001\u5e08\u5f88\u5fd9\uff0c \u5f88\u6709\u53ef\u80fd\u4e0d\u5728 \uff0c\u5982\u679c\u627e\u4e0d\u5230\u8bf7\u548c\u5bfc\u5e08\u534f\u5546\u89e3\u51b3\uff0c\u8fd9\u4e00\u6b65\u7b7e\u5b57\u8bf7\u9884\u7559\u81f3\u5c11\u4e24\u5929\u65f6\u95f4\u3002 \u53ef\u80fd\u51fa\u73b0\u7684\u60c5\u51b5 \u8fdb\u95e8\u4e4b\u540e\u51fa\u95e8\u4e5f\u9700\u8981\u95e8\u7981\u5361\uff0c\u5982\u679c\u6ca1\u6709\u8001\u5e08\u5e2e\u5fd9\u5f00\u95e8\uff0c\u8bf7\u5750\u5728\u5730\u4e0a\u568e\u5555\u5927\u54ed\u3002 \u4fe1\u606f\u529e\u7b7e\u5b57 \u00b6 \u8bbe\u5907\u5904 \u00b6 \u8bbe\u5907\u5904\u7684\u4f4d\u7f6e\u6bd4\u8f83\u96be\u627e\uff0c\u5b9e\u9a8c16\u697c\uff0c\u5728\u5b9e\u9a8c15\u697c\u4e1c\u4fa7\uff0c\u5982\u679c\u4e0d\u5f00\u95e8\u7684\u8bdd\u9700\u8981\u4ece\u540e\u9762\u7ed5\u8fc7\u53bb\u3002 \u5982\u679c15\u697c\u4e5f\u4e0d\u77e5\u9053\u5728\u54ea\uff0c\u8bf7\u95ee\u8def\u4eba\u3002 \u9884\u7ea6\u501f\u6b3e\u5355 \u00b6 \uff08\u8be5\u6761\u76ee\u9700\u8981\u8865\u5145\uff09 Note \u7b7e\u5b57\u76d6\u7ae0\u987a\u5e8f\uff1a \u9884\u7ea6\u501f\u6b3e\u5355\uff1a\u5bfc\u5e08 \u5408\u540c\u5ba1\u6279\u8868\uff1a\u9662\u957f-\u4fe1\u606f\u529e-\u8bbe\u5907\u5904 \u9500\u552e\u5408\u540c\uff1a\u4f9b\u5e94\u5546\u76d6\u7ae0\u90ae\u5bc4-\u8bbe\u5907\u5904-\u5bc4\u56de\u4e00\u4efd \u8be6\u7ec6\u5185\u5bb9\u53ef\u89c1 2.\u5ba1\u6279\u6d41\u7a0b","title":"2. \u5ba1\u6279\u6d41\u7a0b"},{"location":"flow_server_b/#_1","text":"\u8fd9\u4e00\u6b65\u53ef\u80fd\u9700\u8981\u82b1\u8d39\u8d85\u8fc7\u4e00\u5929\u7684\u65f6\u95f4\uff0c\u8bf7\u5408\u7406\u5b89\u6392\u65e5\u7a0b\u3002 \u8bf7\u6309\u4e0b\u9762\u7684\u6b65\u9aa4\u4f9d\u6b21\u529e\u7406\uff0c \u8d8a\u7ea7\u5ba1\u6838 \u662f\u5728\u5404\u4e2a\u5730\u65b9\u90fd\u662f\u4e0d\u88ab\u5141\u8bb8\u7684\u3002","title":"\u6d41\u7a0b\u5ba1\u6279"},{"location":"flow_server_b/#_2","text":"\u627e\u949f\u9662\u957f\uff0c\u7814\u7a76\u751f\u697c9\u697c\u5de6\u624b\u8fb9\u6700\u91cc\u9762\uff0c\u5de6\u624b\u8fb9\u3002 9\u697c\u975e\u8ba1\u7b97\u673a\u4e13\u4e1a\uff0c\u8bbe\u6709\u95e8\u7981\u3002 \u4e0d\u505c\u5730\u6309\u95e8\u94c3\u53ef\u4ee5\u89e3\u51b3\u3002 \u9700\u8981\u6ce8\u610f\uff0c\u949f\u8001\u5e08\u5f88\u5fd9\uff0c \u5f88\u6709\u53ef\u80fd\u4e0d\u5728 \uff0c\u5982\u679c\u627e\u4e0d\u5230\u8bf7\u548c\u5bfc\u5e08\u534f\u5546\u89e3\u51b3\uff0c\u8fd9\u4e00\u6b65\u7b7e\u5b57\u8bf7\u9884\u7559\u81f3\u5c11\u4e24\u5929\u65f6\u95f4\u3002 \u53ef\u80fd\u51fa\u73b0\u7684\u60c5\u51b5 \u8fdb\u95e8\u4e4b\u540e\u51fa\u95e8\u4e5f\u9700\u8981\u95e8\u7981\u5361\uff0c\u5982\u679c\u6ca1\u6709\u8001\u5e08\u5e2e\u5fd9\u5f00\u95e8\uff0c\u8bf7\u5750\u5728\u5730\u4e0a\u568e\u5555\u5927\u54ed\u3002","title":"\u9662\u957f\u7b7e\u5b57"},{"location":"flow_server_b/#_3","text":"","title":"\u4fe1\u606f\u529e\u7b7e\u5b57"},{"location":"flow_server_b/#_4","text":"\u8bbe\u5907\u5904\u7684\u4f4d\u7f6e\u6bd4\u8f83\u96be\u627e\uff0c\u5b9e\u9a8c16\u697c\uff0c\u5728\u5b9e\u9a8c15\u697c\u4e1c\u4fa7\uff0c\u5982\u679c\u4e0d\u5f00\u95e8\u7684\u8bdd\u9700\u8981\u4ece\u540e\u9762\u7ed5\u8fc7\u53bb\u3002 \u5982\u679c15\u697c\u4e5f\u4e0d\u77e5\u9053\u5728\u54ea\uff0c\u8bf7\u95ee\u8def\u4eba\u3002","title":"\u8bbe\u5907\u5904"},{"location":"flow_server_b/#_5","text":"\uff08\u8be5\u6761\u76ee\u9700\u8981\u8865\u5145\uff09 Note \u7b7e\u5b57\u76d6\u7ae0\u987a\u5e8f\uff1a \u9884\u7ea6\u501f\u6b3e\u5355\uff1a\u5bfc\u5e08 \u5408\u540c\u5ba1\u6279\u8868\uff1a\u9662\u957f-\u4fe1\u606f\u529e-\u8bbe\u5907\u5904 \u9500\u552e\u5408\u540c\uff1a\u4f9b\u5e94\u5546\u76d6\u7ae0\u90ae\u5bc4-\u8bbe\u5907\u5904-\u5bc4\u56de\u4e00\u4efd \u8be6\u7ec6\u5185\u5bb9\u53ef\u89c1 2.\u5ba1\u6279\u6d41\u7a0b","title":"\u9884\u7ea6\u501f\u6b3e\u5355"}]}